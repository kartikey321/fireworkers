{
  "name": "fireworkers",
  "version": "0.4.2",
  "description": "Use Cloud Firestore inside Cloudflare Workers.",
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "cloudflare",
    "workers",
    "firebase",
    "firestore",
    "serverless"
  ],
  "author": {
    "name": "Alex Iglesias",
    "url": "https://alexiglesias.me/"
  },
  "homepage": "https://github.com/finsweet/fireworkers#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finsweet/fireworkers.git"
  },
  "bugs": {
    "url": "https://github.com/finsweet/fireworkers/issues"
  },
  "scripts": {
    "build": "node ./bin/build.js",
    "lint": "eslint ./src && prettier --check ./src",
    "lint:fix": "eslint ./src --fix",
    "check": "tsc --noEmit",
    "format": "prettier --write ./src",
    "test": "pnpm playwright test",
    "test:unit": "node --loader ./esbuild-test-loader.mjs --test tests/data-helper.test.ts",
    "test:headed": "pnpm playwright test --headed",
    "release": "changeset publish",
    "update": "pnpm update -i -L -r"
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.2.0",
    "@changesets/cli": "^2.27.12",
    "@eslint/js": "^9.19.0",
    "@finsweet/eslint-config": "^3.0.3",
    "@playwright/test": "^1.50.1",
    "esbuild": "^0.24.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  },
  "dependencies": {
    "crypto-browserify": "^3.12.1",
    "jose": "^5.9.6",
    "stream-browserify": "^3.0.0",
    "superstruct": "^2.0.2",
    "util": "^0.12.5"
  }
}
